<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="payment_method_assign_data">
        <observer name="cash_back_data_assign"
                  instance="Tada\CashbackTracking\Observer\DataAssignObserver"/>
    </event>
    <event name="checkout_submit_all_after">
        <observer name="save_cashback_tracking_entity_after_submit"
                  instance="Tada\CashbackTracking\Observer\SaveCashbackTrackingEntityObserver"/>
    </event>
</config>
