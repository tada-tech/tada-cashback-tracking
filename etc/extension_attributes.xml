<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Api/etc/extension_attributes.xsd">
    <extension_attributes for="Magento\Sales\Api\Data\OrderInterface">
        <attribute code="partner_tracking" type="Tada\CashbackTracking\Api\Data\CashbackTrackingInterface">
            <join reference_table="tada_cashback_tracking" reference_field="order_id" join_on_field="entity_id">
                <field>partner</field>
                <field>partner_parameter</field>
            </join>
        </attribute>
    </extension_attributes>
</config>
